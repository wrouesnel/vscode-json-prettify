{
  "name": "pretty-json-preview",
  "displayName": "Pretty JSON Preview",
  "description": "Instant JSON Prettifier and Previewer",
  "version": "0.0.47",
  "publisher": "Appler",
  "keywords": [
    "json",
    "format",
    "pretty",
    "preview",
    "highlight",
    "jsonl",
    "line"
  ],
  "icon": "resources/icon.png",
  "engines": {
    "vscode": "^1.60.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/appler1009/vscode-json-prettify.git"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "prettyJsonPreview.open",
        "title": "Pretty JSON Preview: Open"
      }
    ]
  },
  "scripts": {
    "lint": "eslint extension.js test/*.js",
    "pretest": "npm run lint",
    "test": "vscode-test",
    "build": "node scripts/generate-themes.js && esbuild ./extension.js --bundle --outfile=dist/extension.js --platform=node --format=cjs --external:vscode",
    "package": "npm run build && vsce package --out pretty-json-preview-${npm_package_version}.vsix",
    "dev-package": "npm version patch -m \"version %s\" && npm install && npm run build && vsce package --out pretty-json-preview-${npm_package_version}.vsix"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.60.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.6",
    "eslint": "^9.9.1",
    "mocha": "^10.8.2",
    "sinon": "^21.0.0",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "highlight.js": "^11.10.0",
    "pino": "^9.7.0"
  }
}
